<?xml version="1.0" encoding="UTF-8"?>
<component
	name="NodeExample"
	extends="Group"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://devtools.web.roku.com/schema/RokuSceneGraph.xsd"
>
	<interface>
		<!-- public fields/functions -->
		<field
			type="string"
			id="state" />
	</interface>

	<script
		type="text/brightscript"
		uri="pkg:/components/NodeExample.brs" />


	<children>
		<Label
			id="nameText"
			text="name" />

		<Button
			id="button1"
			text="button1" />
		<Button
			id="button2"
			text="button2" />
	</children>

</component>
